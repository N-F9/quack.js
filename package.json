		"frameworks",
		"discord-framework"
	],
	"author": {
		"name": "Nick Ferguson",
		"email": "ndferguson@protonmail.com",
		"url": "https://nickf.me"
	},
	"license": "MIT",
	"files": [
		"lib/**/*",
		"global.d.ts",
		"README.md"
	],
	"bugs": {
		"url": "https://github.com/N-F9/quack.js/issues"
	},
